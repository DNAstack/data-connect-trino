{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    ":automergeMinor",
    "schedule:automergeNonOfficeHours",
    "npm:unpublishSafe",
    ":dependencyDashboard"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "major"
      ],
      "labels": [
        "Major"
      ],
      "automerge": false
    }
  ],
  "vulnerabilityAlerts": {
    "labels": [
      "renovate-security"
    ],
    "enabled": true
  },
  "commitMessagePrefix": "[CU-86b4umhm1]",
  "prTitle": "{{{commitMessagePrefix}}} {{{commitMessageAction}}} {{{commitMessageTopic}}} {{{commitMessageExtra}}} {{{commitMessageSuffix}}}",
  "configMigration": true
}
